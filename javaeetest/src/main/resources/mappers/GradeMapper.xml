<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.GradeDao">
    <resultMap id="gradeMap" type="com.bean.Grade">
        <result property="name" column="name"></result>
        <result property="math" column="math"></result>
        <result property="english" column="english"></result>
        <result property="computer" column="computer"></result>
    </resultMap>
    <select id="findAll" resultMap="gradeMap">
        select * from grade;
    </select>
    <select id="findByName" resultMap="gradeMap">
        select * from grade where name=#{name};
    </select>
</mapper>